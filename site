		// Put the play below this line
    fadeUp3.play();
  } else {
  }
});

// Play animation when scrolled into view
$('#title3').on('inview', function(event, isInView) {
  if (isInView) {
		// Put the play below this line
    fadeUp4.play();
  } else {
  }
});

