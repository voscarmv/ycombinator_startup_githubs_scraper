function isAmplitudeLoaded() {
  return !!window.amplitude.runQueuedFunctions
}

function parseUrl(url){
  var a = document.createElement('a');
  a.href = url;
  return {
    href: a.href,
    host: a.host || location.host,
    port: ('0' === a.port || '' === a.port) ? port(a.protocol) : a.port,
    hash: a.hash,
    hostname: a.hostname || location.hostname,
    pathname: a.pathname.charAt(0) != '/' ? '/' + a.pathname : a.pathname,
    protocol: !a.protocol || ':' == a.protocol ? location.protocol : a.protocol,
    search: a.search,
    query: a.search.slice(1)
  };
};

function canonical() {
  var tags = document.getElementsByTagName('link');
  for (var i = 0, tag; tag = tags[i]; i++) {
    if (tag.getAttribute('rel') === 'canonical') {
      return tag.getAttribute('href');
    }
  }
}

function canonicalPath() {
  var canon = canonical();
  if (!canon) return window.location.pathname;
  return parseUrl(canon).pathname;
}

function canonicalUrl(search) {
  var canon = canonical();
  if (canon) return canon.indexOf('?') !== -1 ? canon : canon + search;
  var url = window.location.href;
  var i = url.indexOf('#');
  return i === -1 ? url : url.slice(0, i);
}

function outboundClick(e, eventName) {
  if (!isAmplitudeLoaded()) { return; }
  amplitude.getInstance().logEvent(eventName, null, function() {
    window.location= e.target.href;
  });
  return false;
}

function createProfileClick(e) {
  return outboundClick(e, "Create Profile Clicked");
}

function joinNetworkClick(e) {
  return outboundClick(e, "Join Network Clicked");
}

function applyNowClick(e) {
  return outboundClick(e, "Apply Now Clicked");
}

